{
  "name": "@tinystacks/ops-aws-core-widgets",
  "version": "0.0.12",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinystacks/ops-aws-core-widgets.git"
  },
  "scripts": {
    "build": "npm run build:types; npm run build:js; pushd dist; rm **/*.jsx; popd",
    "build:types": "tsc",
    "build:js": "babel dist --out-dir dist --extensions \".js,.jsx\" --source-maps inline",
    "clean": "rm -rf ./dist || true; rm *.tgz || true; rm -rf ./build;",
    "clean-build": "npm run clean; npm run build",
    "prepack": "npm run clean-build",
    "hard-clean": "npm run clean; rm -rf node_modules; rm package-lock.json",
    "dependency-check": "./node_modules/.bin/depcheck",
    "install-remote": "npm i --@tinystacks:registry=https://registry.npmjs.org",
    "install-local": "npm i --@tinystacks:registry=http:////local-npm-registry:4873",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint-fix": "./node_modules/.bin/eslint --fix ./src",
    "lint-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json ./test",
    "lint-fix-tests": "./node_modules/.bin/eslint --config .eslintrc.test.json --fix ./test",
    "prerelease": "npm version prerelease --preid=local --no-git-tag-version",
    "publish-local": "npm run prerelease; npm publish --tag local --@tinystacks:registry=http://local-npm-registry:4873",
    "qa": "npm run lint && npm run test-cov",
    "setup-test-lint": "node ./scripts/setup-test-lint.js",
    "test": "jest",
    "test-cov": "jest --coverage",
    "test-file": "jest ./test/index.test.ts",
    "test-file-cov": "jest ./testindex.test.ts --coverage",
    "view-test-cov": "jest --coverage || true && open coverage/lcov-report/index.html",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@tinystacks/ops-core": "^0.1.0",
    "@types/jest": "^29.4.0",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isnil": "^4.0.7",
    "@types/react": "^18.0.31",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.52.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.267.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.272.0",
    "@aws-sdk/client-ecs": "^3.278.0",
    "@aws-sdk/client-iam": "^3.276.0",
    "@aws-sdk/client-sts": "^3.319.0",
    "@aws-sdk/types": "^3.257.0",
    "@chakra-ui/icons": "^2.0.17",
    "aws-sdk": "^2.1307.0",
    "chart.js": "^4.2.1",
    "dayjs": "^1.11.7",
    "react-chartjs-2": "^5.2.0",
    "rsuite": "^5.28.1"
  },
  "peerDependencies": {
    "@chakra-ui/react": "2.x",
    "@tinystacks/ops-core": "0.x",
    "@tinystacks/ops-core-widgets": "0.x",
    "@tinystacks/ops-model": "0.x",
    "lodash.get": "4.x",
    "lodash.isempty": "4.x",
    "lodash.isnil": "4.x",
    "react": "18.x"
  }
}
